@model IEnumerable<string>
@{
    ViewData["Title"] = "Index";
}
<h2>Тест WebAPI</h2>
<div class="container">
    <table class="table">
        <thead>
        <tr>
            <th>#</th>
            <th>value</th>
        </tr>
        </thead>
        <tbody>
        @{
            var i = 1;
            foreach (var value in Model)
            {
                <tr>
                    <td>@(i++)</td>
                    <td>@value</td>
                </tr>
            }
        }
        </tbody>
    </table>
</div>